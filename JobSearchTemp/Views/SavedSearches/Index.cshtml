@model IEnumerable<JobSearch.Models.SavedSearch>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CandidateId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JobPostingId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CandidateId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JobPostingId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SavedSearchId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SavedSearchId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SavedSearchId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
